<p id="notice"><%= notice %></p>

<h1>Listing Processors</h1>
<%= button_to 'New Processor', new_processor_path,method: :get %>
<table class="center" border=1>
  <thead>
    <tr>
      <th>Name</th>
      <th>Company</th>
      <th>Speed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @processor.each do |processor| %>
      <tr>
        <td><%= processor.name %></td>
        <td><%= processor.company %></td>
        <td><%= processor.speed %></td>
        <td><%= link_to 'Show', processor %></td>
        <td><%= link_to 'Edit', edit_processor_path(processor) %></td>
        <td><%= link_to 'Destroy', processor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

